{
  "name": "starter-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:svg-sprite": "svg2sprite ./src/assets/svg/sprite ./src/assets/svg/sprite.svg",
    "build:ssr-hml": "ng build --base-href=/ --configuration=hml && ng run atarter-angular:server && node postbuild.js",
    "build:ssr-prd": "ng build --base-href=/ --configuration=prd && ng run atarter-angular:server && node postbuild.js",
    "postbuild": "node postbuild.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.5",
    "@angular/common": "^14.2.5",
    "@angular/compiler": "^14.2.5",
    "@angular/core": "^14.2.5",
    "@angular/forms": "^14.2.5",
    "@angular/platform-browser": "^14.2.5",
    "@angular/platform-browser-dynamic": "^14.2.5",
    "@angular/router": "^14.2.5",
    "@angular/service-worker": "^14.2.5",
    "bootstrap": "^5.1.3",
    "rxjs": "~7.4.0",
    "svg2sprite-cli": " ^2.0.1 ",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular/cli": "^14.2.5",
    "@angular/compiler-cli": "^14.2.5",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.8.4"
  }
}
