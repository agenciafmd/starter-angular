{
  "name": "starter-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run starter-angular:serve-ssr",
    "serve:ssr": "node dist/starter-angular/server/main.js",
    "build:ssr-hml": "ng build --base-href=/ --configuration=hml && ng run starter-angular:server",
    "build:ssr-prd": "ng build --base-href=/ --configuration=prd && ng run starter-angular:server",
    "prerender": "ng run starter-angular:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/platform-server": "^14.2.5",
    "@angular/router": "^14.3.0",
    "@angular/service-worker": "^14.3.0",
    "@nguniversal/express-engine": "^14.2.2",
    "bootstrap": "~5.1.3",
    "express": "^4.18.2",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@nguniversal/builders": "^14.2.3",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^3.10.7",
    "@types/node": "^12.20.55",
    "jasmine-core": "^3.99.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "typescript": "~4.8.4"
  }
}
