{
  "name": "starter-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run starter-angular:serve-ssr",
    "serve:ssr": "node dist/starter-angular/server/main.js",
    "build:ssr-hml": "ng build --base-href=/ --configuration=hml && ng run starter-angular:server",
    "build:ssr-prd": "ng build --base-href=/ --configuration=prd && ng run starter-angular:server",
    "prerender": "ng run starter-angular:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.5",
    "@angular/common": "^15.2.5",
    "@angular/compiler": "^15.2.5",
    "@angular/core": "^15.2.5",
    "@angular/forms": "^15.2.5",
    "@angular/platform-browser": "^15.2.5",
    "@angular/platform-browser-dynamic": "^15.2.5",
    "@angular/platform-server": "^15.2.5",
    "@angular/router": "^15.2.5",
    "@angular/service-worker": "^15.2.5",
    "@nguniversal/express-engine": "^15.2.1",
    "bootstrap": "~5.1.3",
    "express": "^4.18.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.3",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.5",
    "@angular/cli": "^15.2.5",
    "@angular/compiler-cli": "^15.2.5",
    "@nguniversal/builders": "^15.2.1",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^3.10.8",
    "@types/node": "^12.20.55",
    "jasmine-core": "^3.99.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "typescript": "~4.8.4"
  }
}
